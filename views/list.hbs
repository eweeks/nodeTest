{{#if_eq park ':grte'}}
  <h1>GRTE Sites</h1>
    {{#each entries}}
            {{#if_eq Unit_Code 'GRTE'}}
              <h2>{{Sound_Name}}</h2>
              <p>{{Site_Code}}</p>
              <p>{{Tags}}</p>
            {{/if_eq}}
    {{/each}}

{{else}}
    <h1>YEll Sites</h1>
    {{#each entries}}
            {{#if_eq Unit_Code 'YELL'}}
              <h2>{{Sound_Name}}</h2>
              <p>{{Site_Code}}</p>
              <p>{{Tags}}</p>
            {{/if_eq}}
    {{/each}}

{{/if_eq}}
